#include <stdio.h>
void accept (int a[10],int n)
{
    printf("enter the elements of array\n");
    for(int i=0;i<n;i++)
    {
      scanf("%d",&a[i]);
    }
}
void display(int a[10],int n)
{
    printf("elements of array are \n");
    for(int i=0;i<n;i++)
    {
        printf("%d\n",a[i]);
    }
}
int main()
{
    int a[10],n;
    printf("enter number of elements\n");
    scanf("%d",&n);
    accept(a,n);
    display(a,n);
}
