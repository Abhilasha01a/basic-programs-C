#include <stdio.h>
int fib(int n)
{
    if(n==0)
       return 0;
    if(n==1)
       return 1;
    if (n>1)
       return fib(n-1)+fib(n-2);
}
int main()
{
    int n,c,i=0;
    printf("enter the number for fib series\n");
    scanf("%d",&n);
    printf("fib series are\n");
    for(c=0;c<=n;c++)
    {
        printf("%d",fib(c));
        i++;
    }
}
